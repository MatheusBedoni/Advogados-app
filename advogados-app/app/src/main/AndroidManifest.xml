<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.redes.advogadosapp">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".auxiliares.ConstroiAiApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".IntroductionActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ReasonsActivity" />
        <activity
            android:name=".DataActivity"
            android:label="@string/title_activity_data"
            android:theme="@style/AppTheme" />
        <activity android:name=".DataActivity2" />
        <activity android:name=".ConfirmActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".OrderActivity" />
        <activity
            android:name=".OrderDetailsActivity"
            android:label="@string/title_activity_order_details"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>